{
	"Change":{
		"Session":{
			"SaveHandler":{
				"type":"redis",
				"options":{
					"ttl":900,
					"namespace":"franck_dev_sess"
				}
			}
		},
		"Application":{
			"development-mode":true
		},
		"Db":{
			"use": "mysql"
		},
		"I18n":{
			"supported-lcids":[
				"en_US",
				"fr_FR"
			],
			"langs":{
				"en_US":"en",
				"fr_FR":"fr"
			}
		},
		"Install":{
			"webBaseDirectory":"www",
			"webBaseURLPath":""
		},
		"Cache":{
			"block":false
		}
	}
}
